{
  "name": "memevault",
  "version": "1.0.0",
  "description": "MemeVault ist eine Webanwendung zum Erstellen, Teilen und Bewerten von Memes mit Spielmodus",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "postinstall": "npm run install-client",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "setup": "npm install && mkdir -p uploads && echo MONGO_URI=mongodb://localhost:27017/memevault > .env && echo PORT=5000 >> .env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1337Rafael1337/memevault.git"
  },
  "keywords": [
    "meme",
    "generator",
    "react",
    "nodejs",
    "mongodb",
    "game"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.0.7",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  }
}
