{
  "name": "memevault",
  "version": "1.0.0",
  "description": "MemeVault ist eine Webanwendung zum Erstellen, Teilen und Bewerten von Memes. Mit dieser App kannst du eigene Bilder hochladen, Text hinzufÃ¼gen und deine Meme-Kreationen mit anderen teilen.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "postinstall": "npm run install-client",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "setup": "npm install && mkdir -p uploads && echo MONGO_URI=mongodb://localhost:27017/memevault > .env && echo PORT=5000 >> .env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1337Rafael1337/memevault.git"
  },
  "keywords": [
    "meme",
